  "authors": [
    {
      "name": "Krisan Alfa Timur",
      "email": "krisan47@gmail.com"
    }
  ],
  "replace": {
    "krisanalfa/lumen-dingo-adapter": "0.1.0",
    "dolphiq/lumen-dingo-adapter": "1.1",
    "tosinbot/lumen-dingo-adapter": "dev-master",
